# Example environment variables for TicketOffice
# Copy to .env.local and adjust per environment

# Backend API base URL
NEXT_PUBLIC_API_URL=http://localhost:8080

# Toggle frontend mocks
# true  = use mocked data from the frontend services (no backend required)
# false = call the real backend API defined by NEXT_PUBLIC_API_URL
NEXT_PUBLIC_USE_MOCK=true
